import mpi.MPI;

public class mp1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		MPI.Init(args);
		
		int rank = MPI.COMM_WORLD.Rank();
		System.out.println("I'm a process with rank" + rank);
		if(rank == 0) {
			int [] buff = new int[2];
			MPI.COMM_WORLD.Send(buff, 0, 1, MPI.INT, 1, MPI.ANY_SOURCE);()
		}
		
		MPI.Finalize();

	}

}
